{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python",
      "type": "python",
      "request": "launch",
      "pythonPath": "${config.python.pythonPath}",
      "program": "${file}",
      "cwd": "${workspaceRoot}",
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
      ]
    },
    {
      "name": "Python2",
      "type": "python",
      "request": "launch",
      "pythonPath": "C:/Python27/python.exe",
      "program": "${file}",
      "cwd": "${workspaceRoot}",
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
      ]
    }
  ]
}